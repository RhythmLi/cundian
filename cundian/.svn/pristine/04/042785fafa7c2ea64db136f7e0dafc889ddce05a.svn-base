/*-----------------------------首页底部*/
.m-index-nav {
    list-style: none;
    font-size: .25rem;
    text-align: center;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    padding: .05rem 0;
    border-top: 1px solid #e1e1e1;
    background: #ffffff;
    color: #aeaeae;
}

.m-index-nav .g-flex-auto {
    padding-top: .6rem;
    position: relative;
}

.m-index-nav a:before {
    content: "";
    position: absolute;
    width: .6rem;
    top: 0;
    left: 50%;
    margin-left: -.3rem;
    height: .6rem;
}

.m-index-nav .active {
    color: #ff5001;
}

.m-index-nav .m-nav-1 .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -1.8rem -.6rem;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-1.active .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -.6rem 0;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-2 .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat 0 -.6rem;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-2.active .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat 0 0;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-3 .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -6rem -.6rem;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-3.active .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -1.8rem 0;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-4 .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -1.8rem -1.2rem;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-4.active .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -1.2rem 0;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-5 .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -3.6rem -.6rem;
    background-size: 6.6rem;
}

.m-index-nav .m-nav-5.active .nav-list:before {
    background: url("../imgs/sprite2.png") no-repeat -3rem 0;
    background-size: 6.6rem;
}

/*------------------------首页头部*/
.m-header {
    line-height: .9rem;
    background: white;
    font-size: .3rem;
    text-align: center;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
}

.m-header .back {
    width: 4em;
    text-align: center;
}

.m-header .back:before {
    content: "";
    display: inline-block;
    width: .6rem;
    height: .6rem;
    background: url("../imgs/sprite2.png") no-repeat 0 -1.2rem;
    background-size: 6.6rem;
    vertical-align: middle;
}

.m-header .address {
    width: 4em;
}

.m-header .title {
    font-size: .35rem;
}

.m-header .right {
    width: 4em;
    /*background: url("../imgs/index_03.jpg") no-repeat center;*/
    background-size: .6rem;
}

/*==========================底部弹出*/
@-webkit-keyframes alert {
    from {
        opacity: 0;
        /*bottom: 0;*/
        transform:translateY(0);
        -ms-transform:translateY(0); 	/* IE 9 */
        -moz-transform:translateY(0); 	/* Firefox */
        -webkit-transform:translateY(0); /* Safari 和 Chrome */
        -o-transform:translateY(0); 	/* Opera */
    }

    to {
        opacity: 1;
        transform:translateY(-4rem);
        -ms-transform:translateY(-4rem);
        -moz-transform:translateY(-4rem);
        -webkit-transform:translateY(-4rem);
        -o-transform:translateY(-4rem);
    }
}

@keyframes alert {
    from {
        opacity: 0;
        transform:translateY(0);
        -ms-transform:translateY(0);
        -moz-transform:translateY(0);
        -webkit-transform:translateY(0);
        -o-transform:translateY(0);
    }

    to {
        opacity: 1;
        transform:translateY(-4rem);
        -ms-transform:translateY(-4rem);
        -moz-transform:translateY(-4rem);
        -webkit-transform:translateY(-4rem);
        -o-transform:translateY(-4rem);
    }
}

@-webkit-keyframes alert-hide {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@keyframes alert-hide {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

.m-botAlert {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    transform:translateY(-4rem);
    -ms-transform:translateY(-4rem);
    -moz-transform:translateY(-4rem);
    -webkit-transform:translateY(-4rem);
    -o-transform:translateY(-4rem);
    -webkit-animation: alert 1s;
    animation: alert 1s;
    text-align: center;
    font-size: .3rem;
}

.alert-hide {
    opacity: 0;
    -webkit-animation: alert-hide 1s;
    animation: alert-hide 1s;
}

.m-botAlert .m-botAlert-con {
    display: inline-block;
    max-width: 70%;
    word-break: break-all;
    padding: 0.2rem .3rem;
    border-radius: 4px;
    margin: 0 auto;
    background: #333;
    color: #fff;
    line-height: .4rem;
}
